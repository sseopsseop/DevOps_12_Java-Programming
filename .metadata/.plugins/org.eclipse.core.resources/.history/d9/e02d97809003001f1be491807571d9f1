package chap03_operator;

public class _03_LogicalOperator {
	public static void main(String[] args) {
		boolean a = true;
		boolean b = false;
		boolean c;
		c = test2(a) || test2(b);
	}
	
	public static boolean test2(boolean a) {
		if(a == true) 
			System.out.println("true");
		else 
			System.out.println("false");
		return  a;
	}

}
