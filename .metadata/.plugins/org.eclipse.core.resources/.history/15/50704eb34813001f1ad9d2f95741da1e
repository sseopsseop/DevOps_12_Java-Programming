package chap22_lambda;

import java.util.Scanner;

import chap22_lambda.reference.Convertcase;
import chap22_lambda.reference.Converter;

public class _09_ReferenceMethodEx01 {
	public static void main(String[] args) {
		String str;
	}
	
	public static String toConvertCase(Convertcase convertCase) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("문자열 입력 : ");
		String str = sc.nextLine();
		
		convertCase.toConvertcase(str);
		
		sc.close();
	}
}
