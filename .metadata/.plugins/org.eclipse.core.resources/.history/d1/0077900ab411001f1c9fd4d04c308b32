package chap21_multithread.threads;

public class SyncThread02 extends Thread{
	private PrintNumWithSync2 pnws;
	
	public void setPnws(PrintNumWithSync2 pnws) {
		this.pnws = pnws;
	}
	
	public PrintNumWithSync2 getPnws() {
		return this.pnws;
	}
	
	@Override
	public void run() {
		pnws.printNum2(20);
	}
}
