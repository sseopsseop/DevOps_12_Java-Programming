package chap07_class.circle;

public class Circle {

	public final double PI = 3.14;
	poblic int radius;
	
	public Circle() {
		
	}
	public Circle(int radius) {
		this.radius = radius;
	}
	
	public double getRound() {
		return 2*PI*radius;
	}
	
	public double getArea() {
		return PI * radius * radius;
	}
}
