package homework.middle10;

import homework.middle10.classes.Calc;

public class _02_Problem {
	public static void main(String[] args) {
		Double[] dArr = {100.0, 1.0, 2.0, 3.0};
		
		Calc calc = new Calc();
		
		System.out.println(calc.add(dArr));
		System.out.println(calc.sub(dArr));
		System.out.println(calc.mul(dArr));
		System.out.println(calc.div(dArr));
		
	}
}
