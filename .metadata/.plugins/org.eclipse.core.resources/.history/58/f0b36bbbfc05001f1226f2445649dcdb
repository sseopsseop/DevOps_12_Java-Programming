package chap05_array;

public class _02_ArrayExample {
	public static void main(String[] args) {
		// 1. 10개의 정수를 저장하는 배열을 생성하고 3의 배수만 10개 저장하세요.
		
		int[] intArr = new int[10];
		
		for(int i = 0; i < 10; i++) {
			intArr[i] = 3*(i+1);
		}
		
		for(int i =0; i <10; i++) {
			System.out.print(intArr[i] + " ");
		}
		System.out.println();
	}

}
