package chap21_multithread.threads;

public class SyncThread01 extends Thread {
	private PrintNumWithSync pnws;
	
	public void setPnws(PrintNumWithoutSync pnws) {
		this.pnws = pnws;
	}
	
	public PrintNumWithoutSync getPnws() {
		return this.pnws;
	}
	
	@Override
	public void run() {
		pnws.printNum1(10);
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
